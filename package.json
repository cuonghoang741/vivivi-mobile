{
  "name": "native-version",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "npx expo prebuild --skip-dependency-update react-native,react",
    "prebuild:clean": "npx expo prebuild --clean",
    "build:setup": "cd ios && rm -rf Pods Podfile.lock && pod cache clean --all &&  pod install",
    "build:ios": "eas build --platform ios --auto-submit",
    "build:android": "eas build --platform android --auto-submit",
    "build:development": "eas build --profile development --platform ios",
    "build:preview": "eas build --profile preview --platform ios",
    "build:production": "eas build --profile production --platform ios",
    "update:development": "eas update --channel development --message \"Development update\"",
    "update:preview": "eas update --channel preview --message \"Preview update\"",
    "update:production": "eas update --channel production --message \"Production update\"",
    "update:production:ios": "eas update --channel production --message \"Production update\" --platform ios",
    "update:production:android": "eas update --channel production --message \"Production update\" --platform android",
    "build:ios:local": "rm -rf ./*-*.ipa && eas build --local --platform ios --non-interactive && eas submit --platform ios --path $(ls build-*.ipa) --non-interactive",
    "build:android:local": "rm -rf ./*-*.aab && eas build --local --platform android && eas submit --platform android --path $(ls build-*.aab) --non-interactive",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android"
  },
  "dependencies": {
    "@callstack/liquid-glass": "^0.6.0",
    "@expo/ui": "~0.2.0-beta.7",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@supabase/supabase-js": "^2.81.1",
    "dotenv": "^17.2.3",
    "expo": "~54.0.24",
    "expo-apple-authentication": "^8.0.7",
    "expo-asset": "^12.0.10",
    "expo-auth-session": "^7.0.9",
    "expo-crypto": "~15.0.7",
    "expo-file-system": "^19.0.18",
    "expo-font": "~14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-keep-awake": "~15.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.9",
    "expo-liquid-glass-view": "^0.1.9",
    "expo-media-library": "^18.2.0",
    "expo-splash-screen": "^31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "^6.0.8",
    "expo-updates": "^29.0.12",
    "expo-web-browser": "^15.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-purchases": "^9.6.6",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "^4.18.0",
    "react-native-webview": "13.15.0"
  },
  "resolutions": {
    "expo-linear-gradient": "~11.4.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
