{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT": "120",
        "FASTLANE_XCODEBUILD_SETTINGS_RETRIES": "5"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:assembleDebug"
      },
      "channel": "development",
      "autoIncrement": true
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT": "120",
        "FASTLANE_XCODEBUILD_SETTINGS_RETRIES": "5"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "preview",
      "autoIncrement": true
    },
    "production": {
      "env": {
        "FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT": "120",
        "FASTLANE_XCODEBUILD_SETTINGS_RETRIES": "5"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6755465004"
      },
      "android": {
        "releaseStatus": "draft"
      }
    }
  }
}